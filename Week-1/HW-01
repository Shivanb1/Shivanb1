def divisible_five(given_number):
    my_list = []
    for i in range(given_number):
      if i%5 == 0 :
        my_list.append(i) 
    return my_list

given_number = int(input())
divisible_five(given_number)


def my_function(given_number):
  
  lis3=[]
  lis7=[]
  for i in range(given_number):
    if i%3 == 0 :
      lis3.append(i)
    if i%7 ==0:
      lis7.append(i)
    s3=sum(lis3)
    s7=sum(lis7)
    sumf= s3+s7
  return sum


