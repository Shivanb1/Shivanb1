def divisible_five(given_number):
    my_list = []
    m_list=[]
    for i in range(given_number):
      if i%3 == 0 :
        my_list.append(i) 
    s3=sum(my_list)
    for i in range(given_number):
      if i%7 == 0:
        m_list.append(i)
      s7=sum(m_list)
      final=s3+s7
    return final

